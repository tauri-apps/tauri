{
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:4000"
  },
  "tauri": {
    "bundle": {
      "identifier": "studio.tauri.example",
      "active": true
    },
    "allowlist": {
      "all": true
    },
    "windows": [
      {
        "title": "Tauri App"
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self'"
    },
    "updater": {
      "active": false
    }
  }
}
